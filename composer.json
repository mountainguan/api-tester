{
    "name": "mountainguan/api-tester",
    "description": "Api tester for laravel",
    "type": "library",
    "keywords": ["laravel-admin", "api", "tester"],
    "homepage": "https://github.com/mountainguan/api-tester",
    "license": "MIT",
    "authors": [
        {
            "name": "mountainguan",
            "email": "gzs@renaren.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "laravel/framework": ">=5.5,
        "encore/laravel-admin": "1.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "laravel/laravel": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\ApiTester\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\ApiTester\\ApiTesterServiceProvider"
            ]
        }
    }
}
